6> H1 = {house, "123 Maple St"}.
{house,"123 Maple St"}

7> H2 = {house, "456 Maple St"}.
{house,"456 Maple St"}

8> Street = [ H1,H2 | [] ].     %See botton of p. 38
[{house,"123 Maple St"},{house,"456 Maple St"}]

9> [{house, Addr} | Tail] = Street.
[{house,"123 Maple St"},{house,"456 Maple St"}]

10> Addr.
"123 Maple St"

11> f(Addr).    %<=="forget" the Addr variable     
ok

12> [ _,{house, Addr} | [] ] = Street.
[{house,"123 Maple St"},{house,"456 Maple St"}]

13> Addr.
"456 Maple St"

